# Vercel ignore file to exclude large or unnecessary files

# Ignore log files
logs/
*.log

# Ignore data files (these are for local storage only)
data/

# Ignore local Python cache
__pycache__/
*.pyc

# Ignore git-related files
.git/
.gitignore

# Ignore local environment files (but keep .env.example)
.env

# Ignore OS-specific files
.DS_Store
Thumbs.db

# Ignore test databases
*.db